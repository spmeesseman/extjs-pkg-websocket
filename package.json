{
    "name": "@perryjohnson/extjs-pkg-websocket",
    "version": "1.0.6",
    "displayName": "ExtJS Package Wrapper for WebSocket",
    "description": "ExtJS Package Wrapper for WebSocket",
    "license": "MIT",
    "author": {
        "name": "Scott Meesseman",
        "email": "smeesseman@pjats.com"
    },
    "bugs": {
        "email": "smeesseman@pjats.com",
        "url": "https://issues.development.pjats.com/extjs-pkg-websocket/ticket/1"
    },
    "homepage": "https://issues.development.pjats.com/extjs-pkg-websocket",
    "repository": {
        "type": "svn",
        "url": "https://svn.development.pjats.com/repos/pja/extjs-pkg-websocket/trunk"
    },
    "devDependencies": {
        "@perryjohnson/app-publisher": "^1.4.5"
    },
    "scripts": {
        "app-publish": "app-publisher -p ps",
        "app-publish-dry": "app-publisher -p ps --dry-run"
    },
    "keywords": [
        "sencha extjs",
        "websocket",
        "tls1.2"
    ],
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "src"
    ],
    "sencha": {
        "name": "websocket",
        "namespace": "Ext",
        "framework": "ext",
        "type": "code",
        "creator": "spmeesseman",
        "summary": "Package wrapper for WebSocket",
        "detailedDescription": "Package wrapper for WebSocket",
        "version": "1.0.6.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "slicer": null,
        "output": "${framework.dir}/build/packages/${package.name}",
        "local": true,
        "resource": {
            "paths": ""
        },
        "sass": {
            "namespace": "Ext",
            "etc": [
                "${package.dir}/sass/var"
            ],
            "var": [
                "${package.dir}/sass/var"
            ],
            "src": [
                "${package.dir}/sass/src",
                "${package.dir}/sass/src/main.scss"
            ]
        },
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "requires": [
            "core"
        ],
        "ignore": [
            "(^|/)CVS(/?$|/.*?$)"
        ]
    }
}
